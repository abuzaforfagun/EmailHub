{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Servicebus": {
    "PrimaryConnectionString": "Azure connection string",
    "Queues": [
      {
        "Name": "send-email",
        "Contractor": "EmailProcessor.Contracts.SendEmailCommand"
      },
      {
        "Name": "add-log",
        "Contractor": "Logger.Contracts.AddLogCommand"
      }
    ]
  },
  "AllowedHosts": "*"
}
